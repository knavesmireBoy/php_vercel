{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.4"
    }
  },
  "routes": [
    { "src": "/(.*)", "dest": "/api/index.php" },
    { "src": "/public/basic.css", "dest": "/api/public/basic.css" },
    { "src": "/api/public/basic.css", "dest": "/api/public/basic.css" },
    { "src": "/bolt", "dest": "https://www.bbc.co.uk/" }
  ]
}